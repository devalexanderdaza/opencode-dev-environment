{
  "load_variables_from": [
    {
      "variable_loader_type": "dotenv",
      "env_file_path": ".env"
    }
  ],
  "tool_repository": {
    "tool_repository_type": "in_memory"
  },
  "tool_search_strategy": {
    "tool_search_strategy_type": "tag_and_description_word_match"
  },
  "manual_call_templates": [
    {
      "name": "narsil",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "narsil": {
            "transport": "stdio",
            "command": "/Users/michelkerkmeester/bin/narsil-mcp",
            "args": [
              "--repos",
              ".",
              "--index-path",
              ".narsil-index",
              "--git",
              "--call-graph",
              "--persist",
              "--watch",
              "--neural",
              "--neural-backend",
              "api",
              "--neural-model",
              "voyage-code-2"
            ],
            "env": {
              "VOYAGE_API_KEY": "${VOYAGE_API_KEY}",
              "OPENAI_API_KEY": "${OPENAI_API_KEY}",
              "_NOTE_1_NEURAL_BACKENDS": "Three options: 'api' (Voyage/OpenAI), 'onnx' (local, no API needed)",
              "_NOTE_2_VOYAGE_RECOMMENDED": "Default config uses Voyage. Get key: https://dash.voyageai.com/api-keys (starts with 'pa-')",
              "_NOTE_3_OPENAI_ALTERNATIVE": "To use OpenAI instead: change --neural-model to 'text-embedding-3-small'. Get key: https://platform.openai.com/api-keys (starts with 'sk-')",
              "_NOTE_4_LOCAL_ONNX": "For offline/free: change --neural-backend to 'onnx' and remove --neural-model (no API key needed)",
              "_NOTE_5_MODEL_OPTIONS": "Voyage: voyage-code-2, voyage-code-3 | OpenAI: text-embedding-3-small, text-embedding-3-large"
            }
          }
        }
      }
    },
    {
      "name": "github",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "github": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "-y",
              "@modelcontextprotocol/server-github"
            ],
            "env": {
              "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
            }
          }
        }
      }
    },
    {
      "name": "figma",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "figma": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "-y",
              "figma-developer-mcp",
              "--stdio"
            ],
            "env": {
              "FIGMA_API_KEY": "${FIGMA_API_KEY}"
            }
          }
        }
      }
    },
    {
      "name": "clickup",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "clickup": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "-y",
              "@taazkareem/clickup-mcp-server"
            ],
            "env": {
              "CLICKUP_API_KEY": "${CLICKUP_API_KEY}",
              "CLICKUP_TEAM_ID": "${CLICKUP_TEAM_ID}",
              "DOCUMENT_SUPPORT": "true",
              "NODE_ENV": "production"
            }
          }
        }
      }
    },
    {
      "name": "chrome_devtools_1",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "chrome_devtools_1": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "chrome-devtools-mcp@latest",
              "--isolated=true"
            ],
            "env": {}
          }
        }
      }
    },
    {
      "name": "chrome_devtools_2",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "chrome_devtools_2": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "chrome-devtools-mcp@latest",
              "--isolated=true"
            ],
            "env": {}
          }
        }
      }
    },
    {
      "name": "webflow",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "webflow": {
            "transport": "stdio",
            "command": "npx",
            "args": [
              "mcp-remote",
              "https://mcp.webflow.com/sse"
            ],
            "env": {}
          }
        }
      }
    }
  ]
}